{
  "name": "e-commerce-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:dev": "tsc && npm run copy-graphql",
    "copy-graphql": "copyfiles -u 1 src/**/*.graphql dist/src/",
    "build": "nest build && npm run sentry:sourcemaps",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generate": "graphql-codegen --config codegen.ts",
    "start": "nest start",
    "dev2": "ts-node-dev --respawn --transpileOnly src/main.ts",
    "dev": "nest start --watch",
    "start:dev": "nodemon --exec ts-node src/main.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --config ./__test__/jest-unit.json",
    "prepare": "husky install",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org none-wyt --project node-nestjs sentry-artifacts && sentry-cli sourcemaps upload --org none-wyt --project node-nestjs sentry-artifacts"
  },
  "dependencies": {
    "@graphql-codegen/typescript-operations": "^4.4.0",
    "@graphql-codegen/typescript-resolvers": "^4.4.1",
    "@graphql-tools/merge": "^9.0.14",
    "@graphql-tools/schema": "^10.0.13",
    "@nestjs/apollo": "^12.2.2",
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "@nestjs/graphql": "^12.2.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.15",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.15",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@prisma/client": "^6.1.0",
    "@sentry/cli": "^2.39.1",
    "@sentry/nestjs": "^8.47.0",
    "@sentry/node": "^8.47.0",
    "@sentry/profiling-node": "^8.47.0",
    "@types/graphql-type-json": "^0.3.5",
    "apollo-server": "^3.13.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.5.1",
    "cross-spawn": "^7.0.6",
    "google-auth-library": "^9.15.0",
    "graphql": "^16.10.0",
    "graphql-type-json": "^0.3.2",
    "jwks-rsa": "^3.1.0",
    "kafkajs": "^2.2.4",
    "nest-winston": "^1.9.7",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.8.0",
    "prisma-dto-generator": "^1.4.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stripe": "^17.5.0",
    "tar": "^6.2.1",
    "type-graphql": "^2.0.0-rc.2",
    "undici": "^7.2.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.2",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@types/axios": "^0.14.4",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/supertest": "^6.0.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vegardit/prisma-generator-nestjs-dto": "^1.5.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "prisma": "^6.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "watch": [
    "src"
  ],
  "ext": "ts",
  "exec": "ts-node src/main.ts"
}
